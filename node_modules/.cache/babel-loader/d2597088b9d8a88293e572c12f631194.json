A,CAAA,WAAA,CAAA;AACA,aAAA,WAAA,iCACA,KAAA,eADA,+BAEA,KAAA,eAFA,CAAA;AAGA,KA3BA;AA4BA,IAAA,SA5BA,uBA4BA;AACA,aAAA,KAAA,MAAA,CAAA,IAAA;AACA;AA9BA,GAFA;AAkCA,EAAA,UAAA,EAAA;AACA,IAAA,OAAA,EAAA;AADA,GAlCA;AAqCA,EAAA,OArCA,qBAqCA;AACA,SAAA,MAAA,CAAA,QAAA,CAAA,gBAAA,EAAA;AAAA,MAAA,IAAA,EAAA,KAAA;AAAA,KAAA;AACA;AAvCA,CAAA","sourcesContent":["<template>\n  <div class=\"profile-page\" v-if=\"userProfile\">\n    <div class=\"user-info\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-xs-12 col-md-10 offset-md-1\">\n            <img class=\"user-img\" :src=\"userProfile.image\" />\n            <h4>{{ userProfile.username }}</h4>\n            <p>{{ userProfile.bio }}</p>\n            <div>Button</div>\n            <router-link\n              v-if=\"isCurrentuserProfile\"\n              class=\"btn btn-sm btn-outline-secondary action-btn\"\n              :to=\"{ name: 'settings' }\"\n            >\n              Edit Profile Settings\n            </router-link>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12 col-md-10 offset-md-1\">\n          <div class=\"article-toggle\">\n            <ul class=\"nav nav-pills outline-active\">\n              <li class=\"nav-item\">\n                <router-link\n                  :to=\"{\n                    name: 'userProfile',\n                    params: { slug: userProfile.username },\n                  }\"\n                  class=\"nav-link\"\n                  :class=\"{ active: routeName === 'userProfile' }\"\n                >\n                  My Post\n                </router-link>\n              </li>\n              <li class=\"nav-item\">\n                <router-link\n                  :to=\"{\n                    name: 'userProfileFavorites',\n                    params: { slug: userProfile.username },\n                  }\"\n                  class=\"nav-link\"\n                  :class=\"{ active: routeName === 'userProfileFavorites' }\"\n                >\n                  Favorites Posts\n                </router-link>\n              </li>\n            </ul>\n          </div>\n          <mcv-feed :api-url=\"apiUrl\" />\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport McvFeed from \"@/components/Feed\";\nexport default {\n  name: \"McvUserProfile\",\n  computed: {\n    isLoading() {\n      return this.$store.state.userProfile.isLoading;\n    },\n    userProfile() {\n      return this.$store.state.userProfile.data;\n    },\n    error() {\n      return this.$store.state.userProfile.error;\n    },\n    currentUser() {\n      return this.$store.getters.currentUser;\n    },\n    isCurrentuserProfile() {\n      if (!this.currentUser || !this.userProfile) {\n        return false;\n      }\n      return this.currentUser.username === this.userProfile.username;\n    },\n    userProfileSlug() {\n      return this.$route.params.slug;\n    },\n    apiUrl() {\n      const isFavorites = this.$route.path.includes(\"favorites\");\n      return isFavorites\n        ? `/articles?favorited=${this.userProfileSlug}`\n        : `/articles?author=${this.userProfileSlug}`;\n    },\n    routeName() {\n      return this.$route.name;\n    },\n  },\n  components: {\n    McvFeed,\n  },\n  mounted() {\n    this.$store.dispatch(\"getUserProfile\", { slug: this.userProfileSlug });\n  },\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/views"}]}