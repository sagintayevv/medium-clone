n  computed: {\n    pages() {\n      const pagesCount = Math.ceil(this.total / this.limit);\n      return this.range(1, pagesCount);\n    },\n  },\n  methods: {\n    range(start, end) {\n      return [...Array(end).keys()].map((el) => el + start);\n    },\n  },\n};\n</script>\n\n<style>\n</style>"]}]}